/* Gallery 1card = 280px width */
#gallery {
	width: 1120px;
	/*min-width: 810px;
	max-width: 1620px;*/
	margin-right: auto;
	margin-left: auto;
}

.image-card {
	width: 250px;
	height: 230px;
	font-size: 11px;
	margin: 0 15px 0.5em 15px;
	float: left;
}

.image-card img {
	width: 250px;
	max-height: 190px;
	border: 1px solid #CCC;
	padding: 3px;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border-radius: 10px;
	-moz-box-shadow: 1px 1px 1px rgba(0, 0, 0, .1);
	-webkit-box-shadow: 1px 1px 1px rgba(0, 0, 0, .1);
	box-shadow: 1px 1px 1px rgba(0, 0, 0, .1);
}

.image-card .info {
	padding: 0px 10px 0px 10px;
}
.image-card .info .view {
	float: left;
}
.image-card .info .by {
	float: right;
}